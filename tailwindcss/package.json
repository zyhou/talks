{
    "name": "tailwindcss",
    "version": "1.0.0",
    "main": "slides.md",
    "license": "MIT",
    "scripts": {
        "start": "reveal-md slides.md -w",
        "build": "reveal-md slides.md --static public --static-dirs=images",
        "export-pdf": "reveal-md slides.md --print slides.pdf",
        "format": "prettier --write --check \"**/*.+(md|json)\""
    },
    "devDependencies": {
        "prettier": "2.0.5",
        "reveal-md": "4.1.2"
    }
}
